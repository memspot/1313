<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <ImageView
            android:layout_width="412dp"
            android:layout_height="731dp" app:srcCompat="@color/colorAccent" tools:layout_editor_absoluteY="0dp"
            tools:layout_editor_absoluteX="0dp" android:id="@+id/imageView"/>
    <Button
            android:text="Fazer Cadastro"
            android:layout_width="146dp"
            android:layout_height="wrap_content"
            android:id="@+id/button"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            android:layout_marginBottom="164dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:onClick="ClickCadastro" style="@style/Widget.AppCompat.Button.Colored"
            android:background="@color/colorAccent2" app:layout_constraintHorizontal_bias="0.497"/>
    <ImageView
            android:layout_width="336dp"
            android:layout_height="310dp" app:srcCompat="@drawable/folhas2" tools:layout_editor_absoluteY="360dp"
            android:id="@+id/imageView5" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"/>
    <ImageView
            android:layout_width="363dp"
            android:layout_height="278dp" app:srcCompat="@drawable/folhas" tools:layout_editor_absoluteY="131dp"
            android:id="@+id/imageView4" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="302dp" app:srcCompat="@drawable/logopetplant"
            tools:layout_editor_absoluteY="214dp" android:id="@+id/imageView3"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"/>
</android.support.constraint.ConstraintLayout>